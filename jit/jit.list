CPP_FILES = jit.cpp
    
configure
    echo "Configuring @{SUB_DIR}"
    ./conftool "@{PROJECT_COMPILER} -fPIC" "@{SRC_DIR}" "@{SUB_DIR}" "@{CPP_FILES}"

config         : configure
conf           : configure
cf             : configure
    
libjit.so: jit.make_rules @{jit_object_files}
    echo "Linking     libjit.so"
    @{PROJECT_COMPILER} -shared @{jit_object_files} -o libjit.so
